{
  "expo": {
    "name": "ios",
    "slug": "ios",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lunchericon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2D2D3D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ios-ios"
    },
    "android": {
      "package": "com.jinwoong.ios"
    },
    "web": {
      "favicon": "./assets/lunchericon.png"
    }
  }
}
